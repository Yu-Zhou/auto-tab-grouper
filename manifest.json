{
  "manifest_version": 3,
  "name": "Auto Tab Grouper",
  "description": "Automatically groups tabs opened from the same parent page into one tab group.",
  "version": "1.0",
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}