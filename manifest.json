{
  "manifest_version": 3,
  "name": "Auto Tab Grouper",
  "description": "Automatically groups tabs opened from the same parent page into one tab group.",
  "version": "1.0.1",
  "permissions": [
    "tabs",
    "tabGroups",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/logo-16.png",
    "48": "icons/logo-48.png",
    "128": "icons/logo-128.png"
  }
}